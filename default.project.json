{
  "name": "lua-additions",
  "tree": {
    "$className": "DataModel",

    "Workspace": {
      "$className": "Workspace",
      "$properties": {
        "FilteringEnabled": true
      },
      "lua-additions": {
        "$className": "Folder",
        "src": {
          "$path": "src/"
        },
        "tests": {
          "$path": "tests/"
        },
        "MainModule": {
          "$path": "MainModule.lua"
        }
      }
    }
  }
}
